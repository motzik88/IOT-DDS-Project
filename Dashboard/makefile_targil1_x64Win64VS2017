SOURCE_DIR = 

TARGET_ARCH=x64Win64VS2017

# If undefined in the environment default NDDSHOME to install dir
ifndef NDDSHOME
NDDSHOME := C:\Program Files\rti_connext_dds-5.3.1\bin\..
endif

JAVA_PATH = java
JAVAC_PATH = javac

JAVA_SOURCES = $(SOURCE_DIR)./tempSensor.java $(SOURCE_DIR)./tempSensorSeq.java $(SOURCE_DIR)./tempSensorTypeSupport.java $(SOURCE_DIR)./tempSensorTypeCode.java $(SOURCE_DIR)./tempSensorDataReader.java $(SOURCE_DIR)./tempSensorDataWriter.java $(SOURCE_DIR)./startStop.java $(SOURCE_DIR)./startStopSeq.java $(SOURCE_DIR)./startStopTypeSupport.java $(SOURCE_DIR)./startStopTypeCode.java $(SOURCE_DIR)./startStopDataReader.java $(SOURCE_DIR)./startStopDataWriter.java $(SOURCE_DIR)./actuator.java $(SOURCE_DIR)./actuatorSeq.java $(SOURCE_DIR)./actuatorTypeSupport.java $(SOURCE_DIR)./actuatorTypeCode.java $(SOURCE_DIR)./actuatorDataReader.java $(SOURCE_DIR)./actuatorDataWriter.java $(SOURCE_DIR)./actuatorSubscriber.java $(SOURCE_DIR)./actuatorPublisher.java
CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/lib/java/nddsjava.jar

%.class : %.java
	$(JAVAC_PATH)  -classpath ".;$(RTI_CLASSPATH)" $<

all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export PATH := $(NDDSHOME)/lib/x64Win64VS2017;$(PATH)

actuatorPublisher: ./actuatorPublisher.class
	$(JAVA_PATH)  -classpath ".;$(RTI_CLASSPATH)" actuatorPublisher $(ARGS)
 
actuatorSubscriber: ./actuatorSubscriber.class
	$(JAVA_PATH)  -classpath ".;$(RTI_CLASSPATH)" actuatorSubscriber $(ARGS)
